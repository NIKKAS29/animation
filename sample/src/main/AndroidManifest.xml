            android:label="手工动画" />
        <activity
            android:name="hub.hacktons.animation.demo.AnimationXMLActivity"
            android:label="原生动画" />
        <activity
            android:name="hub.hacktons.animation.demo.LottieAnimationActivity"
            android:label="Lottie动画" />
        <activity android:name="hub.hacktons.animation.demo.GifAnimationActivity"></activity>
